{
    "manifest_version": 2,
    "name": "Personalised Starting Page",
    "version": "0.1.0",
    "description": "A personalised starting page",
    "author": "Dennis Borghoff",
    "homepage_url": "https://your-website.com",
    "icons": {
        "48": "icons/icon48.png",
        "96": "icons/icon96.png"
    },
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "public/favicon.ico",
        "default_popup": "index.html"
    }
}